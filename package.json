{
  "name": "PaleGL",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --force --port 8001",
    "dev:watch": "vite --watch",
    "build:page": "tsc && vite build --mode page",
    "build:demo": "tsc && vite build --mode demo",
    "serve:dist": "cd dist && python3 -m http.server 9000",
    "serve:marionetter": "node ./marionetter-server/index.js",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "pack:demo": "node bin/pack.js",
    "build_and_pack:demo": "npm run build:demo && npm run pack:demo"
  },
  "devDependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@babel/types": "^7.27.0",
    "@nabla/vite-plugin-eslint": "^2.0.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/babel-traverse": "^6.25.10",
    "@types/node": "^22.14.0",
    "@types/rollup": "^0.54.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "glob": "^11.0.1",
    "node-zopfli": "^2.1.4",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.39.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "tempy": "^3.1.0",
    "terser": "^5.39.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.5",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-glsl": "^1.4.0",
    "vite-plugin-gltf": "^4.0.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-singlefile": "^2.2.0",
    "vite-plugin-string": "^1.2.3",
    "vite-tsconfig-paths": "^5.1.4",
    "ws": "^8.18.1"
  }
}
